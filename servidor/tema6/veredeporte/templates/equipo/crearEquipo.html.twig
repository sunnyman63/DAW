{% extends 'equipo/equipo.html.twig' %}

{% block title %}Veredeporte - Crear Equipos{% endblock %}

{% block main %}
    <h1 class="text-success">Crea tu propio equipo!!</h1>

    {{ form_start(form , {'attr': {'class':'d-flex flex-column align-items-start'}}) }}

        {{ form_label(form.nombre) }}
        {{ form_widget(form.nombre) }}
        <div class="mt-2">
            {{ form_label(form.tipo) }}
            {{ form_widget(form.tipo) }}
        </div>
        {{ form_widget(form.guardar , {'attr': {'class':'mt-3 btn btn-outline-success'}}) }}

        {{ form_end(form) }}

{% endblock %}